import spacy

# Load the English NER model
nlp = spacy.load("en_core_web_sm")

# Input text
text = "Karthick Balaji is studying AI & Data Science at SIMATS Engineering College in Chennai."

# Process the text
doc = nlp(text)

# Display named entities
print("Named Entities:")
for ent in doc.ents:
    print(ent.text, "->", ent.label_)


OUTPUT
Named Entities:
Karthick Balaji -> PERSON
AI & Data Science at SIMATS Engineering College -> ORG
Chennai -> GPE
